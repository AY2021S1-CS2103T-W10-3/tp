<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane styleClass="pane-with-border" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0" prefWidth="720.0">
         <padding>
            <Insets bottom="5" left="15" right="5" top="5" />
         </padding>
         <children>
            <HBox fx:id="taskDashboardPane" alignment="CENTER_LEFT" prefHeight="91.0" prefWidth="700.0" spacing="5" stylesheets="@Extensions.css">
               <children>
                  <Label id="meetingLabel" fx:id="name" prefHeight="85.0" prefWidth="688.0" styleClass="cell_small_label" stylesheets="@Extensions.css" text="\$first">
                     <font>
                        <Font size="50.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label id="meetingDesc" fx:id="description" prefHeight="85.0" prefWidth="688.0" styleClass="cell_small_label" stylesheets="@Extensions.css" text="\$description">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Label id="meetingPD" fx:id="publishDate" prefHeight="74.0" prefWidth="687.0" styleClass="cell_small_label" stylesheets="@Extensions.css" text="\$publishDate">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Label id="duration" fx:id="duration" prefHeight="82.0" prefWidth="694.0" styleClass="cell_small_label" stylesheets="@Extensions.css" text="\$duration">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Label id="note" fx:id="note" layoutX="25.0" layoutY="271.0" prefHeight="74.0" prefWidth="687.0" styleClass="cell_small_label" stylesheets="@Extensions.css" text="\$note">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</StackPane>
